platform:ios, '9.0'

use_frameworks!

inhibit_all_warnings!           #取消三方框架导致的警告

target 'WYIOSProject' do
  # Pods forWYIOSProject

 pod 'YTKNetwork'
 pod 'YYModel'
 pod 'Masonry'
 pod 'SDWebImage'
 pod 'Reachability'
 pod 'TZImagePickerController'
 pod 'SDCycleScrollView'
 pod 'MOFSPickerManager'
 pod 'FDFullscreenPopGesture'
 pod 'MJExtension'
 pod 'LBXScan/LBXZXing'
 pod 'LBXScan/UI'

end


target 'WYIOSProjectTests' do
    inherit! :search_paths
    # Pods for testing
end

target 'WYIOSProjectUITests' do
    inherit! :search_paths
    # Pods for testing
    end
